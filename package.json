{
  "name": "audit-age",
  "version": "1.0.1",
  "description": "A Node.js CLI and equivalent JS API to audit the age of installed production npm packages.",
  "license": "MIT",
  "author": {
    "name": "Jayden Seric",
    "email": "me@jaydenseric.com",
    "url": "https://jaydenseric.com"
  },
  "repository": "github:jaydenseric/audit-age",
  "homepage": "https://github.com/jaydenseric/audit-age#readme",
  "bugs": "https://github.com/jaydenseric/audit-age/issues",
  "funding": "https://github.com/sponsors/jaydenseric",
  "keywords": [
    "npx",
    "audit",
    "package",
    "dependencies",
    "age",
    "esm",
    "mjs"
  ],
  "files": [
    "cli",
    "private",
    "public"
  ],
  "sideEffects": false,
  "main": "./public/index.mjs",
  "exports": {
    ".": "./public/index.mjs",
    "./public/*": "./public/*",
    "./package.json": "./package.json"
  },
  "bin": {
    "audit-age": "./cli/audit-age.mjs"
  },
  "engines": {
    "node": "^12.22.0 || ^14.17.0 || >= 16.0.0",
    "npm": ">= 7"
  },
  "dependencies": {
    "duration-relativetimeformat": "^2.0.1",
    "kleur": "^4.1.4"
  },
  "devDependencies": {
    "coverage-node": "^6.0.1",
    "eslint": "^8.8.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "jsdoc-md": "^11.0.2",
    "prettier": "^2.5.1",
    "replace-stack-traces": "^1.0.0",
    "snapshot-assertion": "^4.0.1",
    "test-director": "^8.0.1"
  },
  "scripts": {
    "jsdoc": "jsdoc-md",
    "test": "npm run test:eslint && npm run test:prettier && npm run test:jsdoc && npm run test:api",
    "test:eslint": "eslint .",
    "test:prettier": "prettier -c .",
    "test:jsdoc": "jsdoc-md -c",
    "test:api": "coverage-node test/index.mjs",
    "prepublishOnly": "npm test"
  }
}
