{
  "name": "audit-age",
  "version": "1.0.0",
  "description": "A Node.js CLI and equivalent JS API to audit the age of installed production npm packages.",
  "license": "MIT",
  "author": {
    "name": "Jayden Seric",
    "email": "me@jaydenseric.com",
    "url": "https://jaydenseric.com"
  },
  "repository": "github:jaydenseric/audit-age",
  "homepage": "https://github.com/jaydenseric/audit-age#readme",
  "bugs": "https://github.com/jaydenseric/audit-age/issues",
  "funding": "https://github.com/sponsors/jaydenseric",
  "keywords": [
    "npx",
    "audit",
    "package",
    "dependencies",
    "age",
    "esm",
    "mjs"
  ],
  "files": [
    "cli",
    "private",
    "public"
  ],
  "sideEffects": false,
  "main": "./public/index.mjs",
  "exports": {
    ".": "./public/index.mjs",
    "./public/*": "./public/*",
    "./package": "./package.json",
    "./package.json": "./package.json"
  },
  "bin": {
    "audit-age": "./cli/audit-age.mjs"
  },
  "engines": {
    "node": "^12.20 || >= 14.13",
    "npm": ">= 7"
  },
  "dependencies": {
    "duration-relativetimeformat": "^1.0.1",
    "kleur": "^4.1.4"
  },
  "devDependencies": {
    "coverage-node": "^5.0.1",
    "eslint": "^7.32.0",
    "eslint-config-env": "^22.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsdoc": "^36.0.8",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jsdoc-md": "^11.0.2",
    "prettier": "^2.3.2",
    "snapshot-assertion": "^3.0.0",
    "test-director": "^6.0.0"
  },
  "scripts": {
    "jsdoc": "jsdoc-md",
    "test": "npm run test:eslint && npm run test:prettier && npm run test:jsdoc && npm run test:api",
    "test:eslint": "eslint .",
    "test:prettier": "prettier -c .",
    "test:jsdoc": "jsdoc-md -c",
    "test:api": "coverage-node test/index.mjs",
    "prepublishOnly": "npm test"
  }
}
