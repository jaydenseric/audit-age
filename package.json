{
  "name": "audit-age",
  "version": "0.1.1",
  "description": "An npx CLI utility for auditing the age of installed npm packages.",
  "license": "MIT",
  "author": {
    "name": "Jayden Seric",
    "email": "me@jaydenseric.com",
    "url": "https://jaydenseric.com"
  },
  "repository": "github:jaydenseric/audit-age",
  "homepage": "https://github.com/jaydenseric/audit-age#readme",
  "bugs": "https://github.com/jaydenseric/audit-age/issues",
  "funding": "https://github.com/sponsors/jaydenseric",
  "keywords": [
    "npx",
    "audit",
    "package",
    "age"
  ],
  "files": [
    "cli.js"
  ],
  "bin": "cli.js",
  "engines": {
    "node": ">=8.5",
    "npm": ">=5.2"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "cli-table3": "^0.5.0",
    "moment": "^2.22.2"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-env": "^0.1.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.5.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "prettier": "^1.13.5"
  },
  "scripts": {
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier '**/*.md' -l",
    "test": "npm run lint:eslint && npm run lint:prettier",
    "prepublishOnly": "npm test",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": "eslint",
    "*.md": "prettier -l"
  },
  "eslintConfig": {
    "extends": [
      "env"
    ]
  },
  "prettier": {
    "proseWrap": "never",
    "singleQuote": true,
    "semi": false
  }
}
